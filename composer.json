{
  "name": "bluemvc/bluemvc-twig",
  "description": "Twig view renderer for the BlueMvc PHP framework",
  "homepage": "https://bluemvc.com/",
  "license": "MIT",
  "keywords": [
    "blue",
    "mvc",
    "framework",
    "php",
    "twig"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "datatypes/datatypes": "~1.0",
    "bluemvc/bluemvc-core": "dev-master",
    "twig/twig": "~1.28||~2.0"
  },
  "require-dev": {
    "bluemvc/bluemvc-fakes": "dev-master",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "BlueMvc\\Twig\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueMvc\\Twig\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
